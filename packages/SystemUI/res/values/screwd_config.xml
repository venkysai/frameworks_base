    <!-- Snooze: List of snooze values in integer minutes. -->
    <integer-array name="custom_config_notification_snooze_times">
        <item>15</item>
        <item>30</item>
        <item>60</item>
        <item>120</item>
        <item>720</item>
    </integer-array>

     <!-- Tiles native to System UI. Order should match "quick_settings_tiles_default" -->
